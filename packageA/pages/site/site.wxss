.main{
  top: 0;
  bottom: 0;
}
.empty-notice{
  padding-top: 20%;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
}
.notice-icon{
  width: 160rpx;
  height: 160rpx;
  border-radius: 80rpx;
  background: url("data:image/png;base64,") center center no-repeat;
  -webkit-background-size: contain;
  background-size: contain;
  margin: 0 auto;
}
.theme-btn-big:before{
  content: '+ ';
}
.addr-list{
  font-size: 28rpx;
}
.addr-item{
  height: 160rpx;
  position: relative;
}
.addr-desc{
  position: absolute;
  left: 0;
  top: 10rpx;
  right: 160rpx;
  bottom: 10rpx;
  line-height: 56rpx;
  padding-top: 14rpx;
  padding-left: 20rpx;
  border-left: 10rpx solid transparent;
}
.addr-desc.selected{
  border-left: 10rpx solid #ffd600;
}
.addr-edit{
  position: absolute;
  width: 160rpx;
  height: 104rpx;
  top: 30rpx;
  right: 0;
  background: url("data:image/png;base64,") center center no-repeat;
  -webkit-background-size: 44rpx;
  background-size: 44rpx;
  border-left: 1px solid #e0e0e0;
}