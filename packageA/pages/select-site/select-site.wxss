.main{
  top: 0;
}
.title-form{
  border: 1px solid #e0e0e0;
  border-radius: 6rpx;
  height: 90rpx;
  line-height: 90rpx;
  padding-left: 48rpx;
  background: url('data:image/png;base64,') 12rpx center no-repeat; 
   -webkit-background-size: 26rpx;
  background-size: 26rpx;  
  text-align: left;
}
.pois-list{
  position: absolute;
  top:420rpx;
  bottom: 0;
  left: 0;
  right: 0;
  padding-left:30rpx;
  background-color: #fff;
  overflow-y: scroll;
}
.pois-list>view{
  cursor: pointer;
  padding: 10rpx 0;
  font-size: 28rpx;
}
.pois-list>view .pois-item-name{
  line-height: 60rpx; 
}
.pois-list>view:first-child .pois-item-name{
  color:#f40;
  padding-left: 32rpx;
  background: url("data:image/png;base64,") left center no-repeat;
  -webkit-background-size: auto 60%;
  background-size: auto 60%;
}
.sug-list{
  position: absolute;
  top: 90rpx;
  bottom: 0;
  left: 0;
  right: 0;
  padding-left:30rpx;
  background-color: #fff;
  overflow-y: scroll;
}
.sug-list>view{
  cursor: pointer;
  padding: 10rpx 0;
  font-size: 28rpx;
}
.sug-list>view .pois-item-name{
